{
    "name": "wobsoriano/laravel-clerk",
    "description": "Laravel integration for Clerk authentication",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Soriano",
            "email": "sorianorobertc@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "clerkinc/backend-php": "*"
    },
    "autoload": {
        "psr-4": {
            "Wobsoriano\\LaravelClerk\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wobsoriano\\LaravelClerk\\Providers\\ClerkServiceProvider"
            ],
            "aliases": {
                "Clerk": "Wobsoriano\\LaravelClerk\\Facades\\ClerkFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "laravel",
        "clerk",
        "authentication",
        "auth"
    ]
}
